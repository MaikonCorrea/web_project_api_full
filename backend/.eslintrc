{
  "extends": "airbnb-base",
  "rules": {
    "consistent-return": 0,
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "no-unused-vars": ["error", { "argsIgnorePattern": "next","args": "none"}],
    "no-underscore-dangle": [
      "error",
      { "allow": ["_id"] }
    ]
  }
}

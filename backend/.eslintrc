{
  "extends": "airbnb-base",
  "rules": {
        "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "rules": {
      "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
    },
    "no-underscore-dangle": [
      "error",

      {
        "allow": ["_id"]
      }
    ]
  }
}